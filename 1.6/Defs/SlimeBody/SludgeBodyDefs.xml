          <def>Heart</def>
          <customLabel>left vesicle</customLabel>
          <coverage>0.003</coverage>
          <depth>Inside</depth>
          <groups>
            <li>Torso</li>
          </groups>
        </li>
        <li>
          <def>Heart</def>
          <customLabel>lower vesicle</customLabel>
          <coverage>0.003</coverage>
          <depth>Inside</depth>
          <groups>
            <li>Torso</li>
          </groups>
        </li>
        <li>
          <def>Lung</def>
