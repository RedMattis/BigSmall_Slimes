<Defs>

  <GeneDef>
		<defName>BS_SludgeBodySwitcher</defName>
		<label>sludge morphology</label>
		<labelShortAdj>sludge body</labelShortAdj>
		<description>Carriers of this gene have sludgy slime body barely remotely similar to that of baseline humans.</description>
		<biostatCpx>1</biostatCpx>
		<selectionWeight>0</selectionWeight>
		<displayOrderInCategory>430</displayOrderInCategory>
		<iconPath>Icons/bs_slimebody</iconPath>
    <displayCategory>BS_Slime</displayCategory>
    
    <exclusionTags>
      <li>BS_SlimeBody</li>
    </exclusionTags>
    <modExtensions>
			
      <li Class="BigAndSmall.PawnExtension">
				<thingDefSwap>BS_Sludge</thingDefSwap>
			</li>
		</modExtensions>
	</GeneDef>


	<GeneDef>
		<defName>BS_SlimeBodySwitcher</defName>
		<label>slime morphology</label>
		<labelShortAdj>slime body</labelShortAdj>
		<description>Carriers of this gene have slime body that only at best superficially resembles a baseliner.</description>
		<biostatCpx>1</biostatCpx>
		<selectionWeight>0</selectionWeight>
		<displayOrderInCategory>430</displayOrderInCategory>
    <displayCategory>BS_Slime</displayCategory>
		<iconPath>Icons/bs_slimebody_smooth</iconPath>
    
    <exclusionTags>
      <li>BS_SlimeBody</li>
    </exclusionTags>
    <modExtensions>
			
      <li Class="BigAndSmall.PawnExtension">
				<thingDefSwap>BS_Slime</thingDefSwap>
			</li>
		</modExtensions>
	</GeneDef>
</Defs>