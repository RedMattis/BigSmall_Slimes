<Defs>

  <GeneDef>
    <defName>BS_SlimeJump</defName>
    <label>acid bounce</label>
    <description>Carriers of this gene can bounce away long distances leaving acid in the area they land.</description>
    <iconPath>Icons/BS_SlimeAcidJump</iconPath>
    <prerequisite>BS_SlimeResource</prerequisite>
    <biostatCpx>1</biostatCpx>
    <biostatMet>-1</biostatMet>
    <canGenerateInGeneSet>true</canGenerateInGeneSet>
    <displayCategory>BS_Slime</displayCategory>
    <symbolPack>
      <wholeNameSymbols>
        <li><symbol>bouncer</symbol></li>
      </wholeNameSymbols>
      <prefixSymbols>
        <li><symbol>hop</symbol></li>
      </prefixSymbols>
    </symbolPack>
    <abilities>
      <li>BS_SlimeJump</li>
    </abilities>
    <descriptionHyperlinks>
      <AbilityDef>BS_SlimeJump</AbilityDef>
    </descriptionHyperlinks>
    <selectionWeight>1</selectionWeight>

  </GeneDef>

  <GeneDef>
    <defName>BS_SlimeFoamJump</defName>
