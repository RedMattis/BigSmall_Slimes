    <selectionWeight>1</selectionWeight>

  </GeneDef>

  <GeneDef>
    <defName>BS_FireSlimeJump</defName>
    <label>fire bounce</label>
    <description>Carriers of this gene can bounce away long distances igniting the area around where they land.</description>
    <iconPath>GeneIcons/BS_FireJump</iconPath>
    <prerequisite>BS_SlimeResource</prerequisite>
    <biostatCpx>1</biostatCpx>
    <biostatMet>-1</biostatMet>
    <canGenerateInGeneSet>true</canGenerateInGeneSet>
    <displayCategory>BS_Slime</displayCategory>
    <symbolPack>
