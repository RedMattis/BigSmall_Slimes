<Defs>
  <BodyPartDef>
    <defName>BS_SlimeBody</defName>
    <label>slime body</label>
    <hitPoints>70</hitPoints>
    <skinCovered>true</skinCovered>
    <canScarify>true</canScarify>
    <frostbiteVulnerability>0.5</frostbiteVulnerability>
    <tags>
      <li>EatingSource</li>
      <li>TalkingSource</li>
      <li>BloodPumpingSource</li>
    </tags>
  </BodyPartDef>

  <BodyPartDef>
    <defName>BS_SlimeSpine</defName>
    <label>structual elements</label>
    <hitPoints>30</hitPoints>
    <permanentInjuryChanceFactor>6</permanentInjuryChanceFactor>
    <skinCovered>false</skinCovered>
    <solid>true</solid>
    <bleedRate>0</bleedRate>
    <canSuggestAmputation>false</canSuggestAmputation>
    <tags>
      <li>Spine</li>
    </tags>
  </BodyPartDef>

  <BodyPartDef>
    <defName>BS_SlimeBigStomach</defName>
    <label>stomach</label>
    <hitPoints>30</hitPoints>
    <skinCovered>false</skinCovered>
    <tags>
      <li>MetabolismSource</li>
      <li>EatingSource</li>
    </tags>
  </BodyPartDef>
  
  <BodyDef>
    <defName>BS_SlimeBody</defName>
    <label>sapient slime</label>
    <corePart>
      <def>BS_SlimeBody</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <groups>
        <li>Torso</li>
        <li>Teeth</li>
        <li>Mouth</li>
      </groups>
      <parts>
        <li>
          <def>Brain</def>
          <customLabel>slime core</customLabel>
          <coverage>0.02</coverage>
          <groups>
            <li>Torso</li>
          </groups>
        </li>
        <li>
          <def>BS_SlimeSpine</def>
          <coverage>0.001</coverage>
          <depth>Inside</depth>
          <groups>
            <li>Torso</li>
          </groups>
        </li>
        <li>
          <def>BS_SlimeBigStomach</def>
          <customLabel>nutrient chamber</customLabel>
          <coverage>0.075</coverage>
          <depth>Inside</depth>
          <groups>
            <li>Torso</li>
          </groups>
        </li>
        <li>
          <def>Heart</def>
          <customLabel>right vesicle</customLabel>
          <coverage>0.004</coverage>
          <depth>Inside</depth>
          <groups>
            <li>Torso</li>
          </groups>
        </li>
        <li>
          <def>Heart</def>
          <customLabel>left vesicle</customLabel>
          <coverage>0.004</coverage>
          <depth>Inside</depth>
          <groups>
            <li>Torso</li>
          </groups>
        </li>
        <li>
          <def>Lung</def>
          <customLabel>left aerogel sac</customLabel>
          <coverage>0.025</coverage>
          <depth>Inside</depth>
          <groups>
            <li>Torso</li>
          </groups>
        </li>
        <li>
          <def>Lung</def>
          <customLabel>right aerogel sac</customLabel>
          <coverage>0.025</coverage>
          <depth>Inside</depth>
          <groups>
            <li>Torso</li>
          </groups>
        </li>
        <li>
          <def>Kidney</def>
          <customLabel>left purification pod</customLabel>
          <coverage>0.017</coverage>
          <depth>Inside</depth>
          <groups>
            <li>Torso</li>
          </groups>
        </li>
        <li>
          <def>Kidney</def>
          <customLabel>right purification pod</customLabel>
          <coverage>0.017</coverage>
          <depth>Inside</depth>
          <groups>
            <li>Torso</li>
          </groups>
        </li>
        <li>
          <def>Liver</def>
          <customLabel>nutrient core</customLabel>
          <coverage>0.025</coverage>
          <depth>Inside</depth>
          <groups>
            <li>Torso</li>
          </groups>
        </li>
        <li>
          <def>Neck</def>
          <coverage>0.075</coverage>
          <height>Top</height>
          <groups>
            <li>Neck</li>
          </groups>
          <parts>
            <li>
              <def>Head</def>
              <coverage>0.80</coverage>
              <groups>
                <li>UpperHead</li>
                <li>FullHead</li>
                <li>HeadAttackTool</li>
              </groups>
              <parts>
                <li>
                  <def>Eye</def>
                  <customLabel>left eye</customLabel>
                  <coverage>0.07</coverage>
                  <groups>
                    <li>FullHead</li>
                    <li>Eyes</li>
                  </groups>
                  <woundAnchorTag>LeftEye</woundAnchorTag>
                  <flipGraphic>true</flipGraphic>
                </li>
                <li>
                  <def>Eye</def>
                  <customLabel>right eye</customLabel>
                  <coverage>0.07</coverage>
                  <groups>
                    <li>FullHead</li>
                    <li>Eyes</li>
                  </groups>
                  <woundAnchorTag>RightEye</woundAnchorTag>
                </li>
                
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>Shoulder</def>
          <customLabel>left shoulder</customLabel>
          <coverage>0.12</coverage>
          <woundAnchorTag>LeftShoulder</woundAnchorTag>
          <groups>
            <li>Shoulders</li>
          </groups>
          <flipGraphic>true</flipGraphic>
          <parts>
            <li>
              <def>Arm</def>
              <customLabel>left arm</customLabel>
              <coverage>0.77</coverage>
              <groups>
                <li>Arms</li>
              </groups>
              <flipGraphic>true</flipGraphic>
              <parts>
                <li>
                  <def>Hand</def>
                  <customLabel>left hand</customLabel>
                  <coverage>0.14</coverage>
                  <height>Bottom</height>
                  <groups>
                    <li>Hands</li>
                    <li>LeftHand</li>
                  </groups>
                  <flipGraphic>true</flipGraphic>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>Shoulder</def>
          <customLabel>right shoulder</customLabel>
          <coverage>0.12</coverage>
          <woundAnchorTag>RightShoulder</woundAnchorTag>
          <groups>
            <li>Shoulders</li>
          </groups>
          <parts>
            <li>
              <def>Arm</def>
              <customLabel>right arm</customLabel>
              <coverage>0.77</coverage>
              <groups>
                <li>Arms</li>
              </groups>
              <parts>
                <li>
                  <def>Hand</def>
                  <customLabel>right hand</customLabel>
                  <coverage>0.14</coverage>
                  <height>Bottom</height>
                  <groups>
                    <li>Hands</li>
                    <li>RightHand</li>
                  </groups>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>Waist</def>
          <coverage>0</coverage>
          <height>Bottom</height>
          <groups>
            <li>Waist</li>
          </groups>
        </li>
        <li>
          <def>Leg</def>
          <customLabel>left leg</customLabel>
          <coverage>0.14</coverage>
          <height>Bottom</height>
          <groups>
            <li>Legs</li>
          </groups>
          <woundAnchorTag>LeftLeg</woundAnchorTag>
          <flipGraphic>true</flipGraphic>
          <parts>
            <li>
              <def>Foot</def>
              <customLabel>left foot</customLabel>
              <coverage>0.1</coverage>
              <groups>
                <li>Feet</li>
              </groups>
              <flipGraphic>true</flipGraphic>
            </li>
          </parts>
        </li>
        <li>
          <def>Leg</def>
          <customLabel>right leg</customLabel>
          <coverage>0.14</coverage>
          <height>Bottom</height>
          <groups>
            <li>Legs</li>
          </groups>
          <woundAnchorTag>RightLeg</woundAnchorTag>
          <parts>
            <li>
              <def>Foot</def>
              <customLabel>right foot</customLabel>
              <coverage>0.1</coverage>
              <groups>
                <li>Feet</li>
              </groups>
            </li>
          </parts>
        </li>
      </parts>
    </corePart>
  </BodyDef>
</Defs>